<main>
  <div class="container">
    {{> form this }}
  </div>

  <div class="container">
    <h2>Validating {{validate.url}}</h2>
  </div>

  <div class="contrast-map js-contrast-map">
    <ul class="contrast-map__legend">
      <li><span class="contrast-map__legend-level--aaa">Green</span>: WCAG level AAA</li>
      <li><span class="contrast-map__legend-level--aa">Orange</span>: WCAG level AA</li>
      <li><span class="contrast-map__legend-level--a">Other</span>: WCAG level A</li>
    </ul>

    <img src="{{validate.screenshot}}" alt="" class="contrast-map__image">
  </div>
</main>

<script src="/assets/scripts/app.js"></script>
